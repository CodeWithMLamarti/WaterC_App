<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Page_Navigation_App.ViewModel"
                    xmlns:view="clr-namespace:Page_Navigation_App.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ArchiveVM}">
        <view:Archive/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AboutVM}">
        <view:About/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TestVM}">
        <view:Test/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StatisticsVM}">
        <view:Statistics/>
    </DataTemplate>
    
</ResourceDictionary>